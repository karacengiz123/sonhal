{% extends 'base.html.twig' %}

{% block title %}HoldLog{% endblock %}

{% block body %}
    <h1>HoldLog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ hold_log.id }}</td>
            </tr>
            <tr>
                <th>StartTime</th>
                <td>{{ hold_log.startTime ? hold_log.startTime|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndTime</th>
                <td>{{ hold_log.endTime ? hold_log.endTime|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UniqueId</th>
                <td>{{ hold_log.uniqueId }}</td>
            </tr>
            <tr>
                <th>CallType</th>
                <td>{{ hold_log.callType }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ hold_log.duration }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('hold_log_index') }}">back to list</a>

    <a href="{{ path('hold_log_edit', {'id': hold_log.id}) }}">edit</a>

    {{ include('hold_log/_delete_form.html.twig') }}
{% endblock %}
