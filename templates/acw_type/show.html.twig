{% extends 'layout/base.html.twig' %}


{% block title %}AcwType{% endblock %}

{% block body %}
    <h1>AcwType</h1>
<div style="padding: 20px;background-color: white">
    <a href="{{ path('acw_type_index') }}" style="float: right!important; margin-right: 10px;margin-bottom: 20px "><button type="button" class="btn  btn-info" >Listeye Geri Dön</button></a>

    <table id="breakTypeTable" class="table table-striped table-bordered" style="width:100%">
        <tbody>

            <tr>
                <th>İsim</th>
                <td>{{ acw_type.name }}</td>
            </tr>
            <tr>
                <th>Role</th>
                <td>{{ acw_type.role }}</td>
            </tr>
        </tbody>
    </table>
</div>
    {% if acw_type.id not in [1, 2, 3] %}
        <a style="float: right!important; margin-right: 10px;"><button type="button" class="btn  btn-warning"  href="{{ path('acw_type_edit', {'id': acw_type.id}) }}">Güncelle</button></a>
    {% endif %}
<div style="float: right!important; margin-right: 10px;">
    {% if acw_type.id not in [1, 2, 3] %}
        {{ include('acw_type/_delete_form.html.twig') }}
    {% endif %}
</div>
{% endblock %}
