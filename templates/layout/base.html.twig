    <script>
        CKEDITOR.replace( 'wall_messages[wallmessages]' );

        function wallMessageAddFunction() {
            $.ajax({
                url: "{{ path('add_message') }}",
                method: "POST",
                data:{
                    message: CKEDITOR.instances.wallMessageName.getData()
                },
                success:function (data) {
                    if (data.success === true){
                        $("#wallSuccesMessage").html("Kayıt Başarılı");
                    }else {
                        $("#wallSuccesMessage").html("Kayıt Başarısız")
                    }
                    setTimeout(function () {
                        wallModalStop();
                    },3000);
                },
                error:function (data) {
                    $("#wallSuccesMessage").html("Kayıt Başarısız");
                    setTimeout(function () {
                        wallModalStop();
                    },3000);
                }
            })
        }

        function wallMessageClear(setData = "") {
            $("#wallSuccesMessage").html("");
            CKEDITOR.instances.wallMessageName.setData(setData);
        }

        function wallModalStart() {
            $.ajax({
                url: "{{ path('old_message') }}",
                method: "GET",
                success:function (data) {
                    wallMessageClear(data.oldMessage);
                    $("#wallboardModal").modal({
                        backdrop:'static',
                        keyboard: false,
                        show: true,
                        escapeClose: false,
                        clickClose: false,
                        showClose: false
                    });
                },
                error:function (data) {
                    wallMessageClear();
                    $("#wallboardModal").modal({
                        backdrop:'static',
                        keyboard: false,
                        show: true,
                        escapeClose: false,
                        clickClose: false,
                        showClose: false
                    });
                }
            });
        }

        function wallModalStop() {
            $("#wallboardModal").modal('hide');
            wallMessageClear();
        }


    </script>

