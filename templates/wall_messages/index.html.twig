<!DOCTYPE html>

<title>WallMessages index</title>

{% block body %}
    <h1>WallMessages index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Wallmessages</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for wall_message in wall_messages %}
            <tr>
                <td>{{ wall_message.id }}</td>
                <td>{{ wall_message.wallmessages }}</td>
                <td>
                    <a href="{{ path('wall_messages_show', {'id': wall_message.id}) }}">show</a>
                    <a href="{{ path('wall_messages_edit', {'id': wall_message.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('wall_messages_new') }}">Create new</a>
{% endblock %}
