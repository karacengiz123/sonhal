{% extends 'layout/base.html.twig' %}


{% block body %}



    {{ form_start(form) }}

    <div class="form-group">
        <p class="control-label col-sm-2">Açıklama</p>
        <div class="col-sm-10">
            {{ form_widget(form.wallmessages) }}
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button class="btn">{{ button_label|default('KAYDET') }}</button>
        </div>
    </div>
    {{ form_end(form) }}



{% endblock %}



{% block customScript %}

    <script>

        CKEDITOR.replace( 'wall_messages[wallmessages]' );

    </script>
{% endblock %}
