{% extends 'base.html.twig' %}

{% block title %}AcwLog index{% endblock %}

{% block body %}
    <h1>AcwLog index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>StartTime</th>
                <th>EndTime</th>
                <th>Duration</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for acw_log in acw_logs %}
            <tr>
                <td>{{ acw_log.id }}</td>
                <td>{{ acw_log.startTime ? acw_log.startTime|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ acw_log.endTime ? acw_log.endTime|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ acw_log.duration }}</td>
                <td>
                    <a href="{{ path('acw_log_show', {'id': acw_log.id}) }}">show</a>
                    <a href="{{ path('acw_log_edit', {'id': acw_log.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('acw_log_new') }}">Create new</a>
{% endblock %}
