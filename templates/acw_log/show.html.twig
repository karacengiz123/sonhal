{% extends 'base.html.twig' %}

{% block title %}AcwLog{% endblock %}

{% block body %}
    <h1>AcwLog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ acw_log.id }}</td>
            </tr>
            <tr>
                <th>StartTime</th>
                <td>{{ acw_log.startTime ? acw_log.startTime|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndTime</th>
                <td>{{ acw_log.endTime ? acw_log.endTime|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ acw_log.duration }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('acw_log_index') }}">back to list</a>

    <a href="{{ path('acw_log_edit', {'id': acw_log.id}) }}">edit</a>

    {{ include('acw_log/_delete_form.html.twig') }}
{% endblock %}
