                    <div id="tree"></div>
                    <script>
                        var tree = $('#tree').tree({
                            primaryKey: 'id',
                            dataSource: [
                                {% for agent in agents %}
                                {text: '{{ agent.callerId }}', children: [
                                    { text: '{{ agent.cusId }}' },
                                    ]
                                },
                                {% endfor %}
                            ],
                            imageUrlField: 'flagUrl'
                        });
